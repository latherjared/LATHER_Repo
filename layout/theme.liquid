 <script>
   $(document).ready(function() {
	var s = $("#shopify-section-header");
	var pos = s.position();					   
	$(window).scroll(function() {
		var windowpos = $(window).scrollTop();
		if (windowpos >= pos.top & windowpos >=500) {
          s.addClass("sticknav");
		} else {
			s.removeClass("sticknav");
		}
	});
});
 </script>
  <script>
  $(document).ready(function(){
    $('.link_detail').find('li:first').addClass('hulhula');
    setTimeout(function(){
      $('.hulhula').trigger('click');
      $('.hulhula p').addClass('show');
      $('.hulhula p').css('display','block');
      	
    },1000);
    
    
  	$('.tabs li p').hide();
    $( ".gridlock" ).on('click','.tabs li h4',function(){
       $('.tabs li p').addClass('close');
      if(  $(this).parents('li').find( "p" ).hasClass('show')){         
          var showthis = 0;
      }
      else{
        
          var showthis = 1;        
      }
     $('.tabs li p.close').hide('slow');
      $('.tabs li p').removeClass('show');
      if(showthis == 1){
        $(this).parents('li').find( "p" ).show( "slow");
        $(this).parents('li').find( "p" ).addClass('show');
          $(this).parents('li').find( "p" ).removeClass('close');
      }else{
      	$(this).parents('li').find( "p" ).hide( "slow");
         $(this).parents('li').find( "p" ).addClass('close');
          $(this).parents('li').find( "p" ).removeClass('show');
        }
       //$('.tabs li p.show').show('slow');
    });
     $(".tabs li").click(function(){
       	if($(this).hasClass('active')){
          $(this).removeClass("active");
        }else{
          $(".tabs li").removeClass("active");
        	$(this).addClass("active");
        }
    });	
  });

</script>
